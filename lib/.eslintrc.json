/*
VSCE: ESLint
VSCE: Sort JS object keys
VSCE: JavaScript and TypeScript Nightly
*/
{
	"env": {
		"browser": true,
		"es2021": true,
		"node": true
	},
	"extends": ["plugin:lube/recommended"],
	"ignorePatterns": ["*.min.js", "history/**/*.js"],
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"rules": {
		"prefer-const": "off"
	}
}