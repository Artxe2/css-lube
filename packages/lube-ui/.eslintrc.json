/*
VSCE: ESLint
VSCE: Sort JS object keys
VSCE: JavaScript and TypeScript Nightly
*/
{
	"env": {
		"browser": true,
		"es2021": true,
		"node": true
	},
	"extends": ["plugin:lube/recommended"],
	"overrides": [{
		"files": ["*.svelte"],
		"parser": "svelte-eslint-parser",
		"parserOptions": {
			"parser": "@typescript-eslint/parser"
		}
	}],
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"rules": {}
}