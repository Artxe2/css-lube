{
	"author": "artxe2",
	"description": "Svelte UI Components for CSS Lube",
	"devDependencies": {
		"@typescript-eslint/parser": "^6.12.0",
		"eslint": "^8.54.0",
		"eslint-plugin-lube": "^0.4.3",
		"svelte": "5.0.0-next.9",
		"svelte-check": "^3.6.0",
		"svelte-eslint-parser": "^0.33.1",
		"typescript": "^5.3.2"
	},
	"exports": {
		".": {
			"import": "./src/index.js",
			"types": "./types/index.d.ts"
		},
		"./typography": {
			"default": "./src/typography/index.js",
			"types": "./types/typography.d.ts"
		},
		"./utils": {
			"default": "./src/utils/index.js",
			"types": "./types/utils.d.ts"
		}
	},
	"keywords": [],
	"license": "MIT",
	"main": "src/index.js",
	"name": "lube-ui",
	"scripts": {
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"lint": "eslint --cache --fix **/*.{js,json,ts,svelte}"
	},
	"type": "module",
	"types": "types/index.d.ts",
	"typesVersions": {
		"*": {
			"*": [ "types/index.d.ts" ],
			"typography": [ "types/typography.d.ts" ],
			"utils": [ "types/utils.d.ts" ]
		}
	},
	"version": "0.1.0"
}