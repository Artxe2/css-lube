/*
VSCE: ESLint
VSCE: Sort JS object keys
VSCE: JavaScript and TypeScript Nightly
*/
{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": ["plugin:lube/recommended"],
	"ignorePatterns": ["*.min.js", "*.d.ts"],
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": ["lube"],
	"rules": {
		"prefer-const": "off",
		"no-undef": "off",
		"no-unused-vars": "off"
	}
}